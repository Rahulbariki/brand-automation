{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/dashboard",
            "destination": "/frontend/dashboard.html"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/frontend/assets/$1"
        },
        {
            "source": "/static/(.*)",
            "destination": "/frontend/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}